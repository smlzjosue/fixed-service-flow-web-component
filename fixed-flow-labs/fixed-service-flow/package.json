{
  "name": "@smlzjosue/fixed-service-flow",
  "version": "1.2.10",
  "description": "Web Component para solicitud de servicio fijo empresarial - Claro Puerto Rico",
  "repository": {
    "type": "git",
    "url": "https://github.com/smlzjosue/fixed-service-flow-web-component.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/smlzjosue/fixed-service-flow-web-component#readme",
  "bugs": {
    "url": "https://github.com/smlzjosue/fixed-service-flow-web-component/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fixed-service-flow/fixed-service-flow.esm.js",
  "exports": {
    ".": {
      "import": "./dist/fixed-service-flow/fixed-service-flow.esm.js",
      "require": "./dist/fixed-service-flow/fixed-service-flow.cjs.js"
    },
    "./fixed-service-flow": {
      "import": "./dist/components/fixed-service-flow.js",
      "types": "./dist/components/fixed-service-flow.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/",
    "src/assets/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "generate": "stencil generate",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\""
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@stencil/core": "^4.27.1",
    "@stencil/sass": "^3.2.3",
    "@stencil/store": "^2.2.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.7.4",
    "puppeteer": "^24.3.0"
  },
  "keywords": [
    "stencil",
    "web-component",
    "claro",
    "puerto-rico",
    "fixed-service"
  ],
  "author": "E1",
  "license": "MIT"
}
